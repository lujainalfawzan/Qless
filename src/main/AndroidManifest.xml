<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qless2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Qless"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Qless"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewRestList" />
        <activity android:name=".ViewCafeList" />
        <activity android:name=".ViewNewP" />
        <activity android:name=".ShopMenu" />
        <activity android:name=".ViewFood" />
        <activity android:name=".ViewDrink" />
        <activity android:name=".CartActivity" />
        <activity android:name=".cc" />
    </application>

</manifest>